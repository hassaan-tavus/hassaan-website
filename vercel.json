{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/create-video-call",
      "dest": "api/index.js"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/interests",
      "dest": "/public/interests.html"
    },
    {
      "src": "/digitaltwin",
      "dest": "/public/digitaltwin.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}